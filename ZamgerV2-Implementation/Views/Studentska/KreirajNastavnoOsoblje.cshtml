            <div class="page-content">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            <div class="page-title">
                                <div class="row">
                                    <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                                        <div class="Page-title">Podaci osobe</div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-body">
                                <form class="custom-mt-form" asp-antiforgery="false" asp-action="KreirajNastavnoOsoblje" method="post">
                                    <div class="row">
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-12">

                                            <div class="form-group custom-mt-form-group">
                                                <input type="text" name="ime">
                                                <label class="control-label">Ime</label><i class="bar"></i>
                                            </div>

                                            <div class="form-group custom-mt-form-group">
                                                <input class="datetimepicker" type="text" name="datumRodjenja">
                                                <label class="control-label">Datum rođenja</label><i class="bar"></i>
                                            </div>
                                            <div class="form-group custom-mt-form-group">
                                                <select name="odsjekOdabir">
                                                    <option>Računarstvo i informatika</option>
                                                    <option>Elektroenergetika</option>
                                                    <option>Automatika i elektronika</option>
                                                    <option>Telekomunikacije</option>
                                                </select>
                                                <label class="control-label">Odsjek</label><i class="bar"></i>
                                            </div>
                                            <div class="form-group custom-mt-form-group">
                                                <select name="prviPredmetOdabir">
                                                    <option value="-1">Izaberite</option>
                                                    @foreach (KeyValuePair<string, int> entry in ViewBag.Predmeti)
                                                    {
                                                        <option value="@entry.Value">@entry.Key</option>
                                                    }
                                                </select>
                                                <label class="control-label">Predmet 1</label><i class="bar"></i>
                                            </div>

                                            <div class="form-group custom-mt-form-group">
                                                <select name="spol">
                                                    <option>M</option>
                                                    <option>Ž</option>
                                                </select>
                                                <label class="control-label">Spol</label><i class="bar"></i>
                                            </div>


                                        </div>
                                        <div class="col-lg-6 col-md-6 col-sm-6 col-12">

                                            <div class="form-group custom-mt-form-group">
                                                <input type="text" name="prezime">
                                                <label class="control-label">Prezime</label><i class="bar"></i>
                                            </div>
                                            <div class="form-group custom-mt-form-group">
                                                <input type="text" name="prebivaliste">
                                                <label class="control-label">Mjesto prebivališta</label><i class="bar"></i>
                                            </div>
                                            <div class="form-group custom-mt-form-group">
                                                <select name="titulaOdabir">
                                                    <option>Red. prof. dr</option>
                                                    <option>Doc. dr</option>
                                                    <option>Van. prof. dr</option>
                                                    <option>Mr. dipl. ing</option>
                                                    <option>BSc. ing</option>
                                                </select>
                                                <label class="control-label">Titula</label><i class="bar"></i>
                                            </div>
                                            <div class="form-group custom-mt-form-group">
                                                <select name="drugiPredmetOdabir">
                                                    <option value="-1">Izaberite</option>
                                                    @foreach (KeyValuePair<string, int> entry in ViewBag.Predmeti)
                                                    {
                                                        <option value="@entry.Value">@entry.Key</option>
                                                    }
                                                </select>
                                                <label class="control-label">Predmet 2</label><i class="bar"></i>
                                            </div>
                                        </div>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-12">

                                            <div class="form-group text-center custom-mt-form-group">
                                                <button class="btn btn-primary mr-2" type="submit">Kreiraj</button>
                                                <button class="btn btn-secondary" type="reset">Odustani</button>
                                            </div>

                                        </div>
                                    </div>
                                </form>
                            </div>
                            </div>
                        </div>
                    </div>
                </div>
